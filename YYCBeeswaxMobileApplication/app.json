{
    "expo": {
        "name": "YYCBeeswaxMobileApplication",
        "slug": "YYCBeeswaxMobileApplication",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/YYCBeeswaxIcon.png",
        "scheme": "bees",
        "userInterfaceStyle": "light",
        "jsEngine": "hermes",
        "splash": {
            "image": "./assets/YYCBeeswaxFullLogo.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/YYCBeeswaxIcon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.anonymous.bee"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "tsconfigPaths": true,
            "typedRoutes": true
        }
    }
}
